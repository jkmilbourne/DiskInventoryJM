@model List<Artist>
@{
    ViewData["Title"] = "Artist Page";
}

<h2>Artist List</h2>

@*<a asp-controller="Movie" asp-action="Add">Add New Movie</a>*@
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Artist Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Artist Type</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var artist in Model)
        {
            <tr>
                <td>@artist.ArtistId</td>
                <td>@artist.ArtistFname</td>
                <td>@artist.ArtistLname</td>
                <td>@artist.ArtistTypeCode</td>
                @*<td>
                    <a asp-controller="Movie" asp-action="Edit"
                       asp-route-id="@movie.MovieId"
                       asp-route-slug="@movie.Slug">Edit</a>
                    <a asp-controller="Movie" asp-action="Delete"
                       asp-route-id="@movie.MovieId"
                       asp-route-slug="@movie.Slug">Delete</a>
                </td>*@
            </tr>
        }
    </tbody>
</table>
